<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ccit.model.Bundle" table="t_bundle">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="name" column="b_name" />
		<property name="update" column="b_update" />
		<property name="modifydate" column="b_modifydate" />
		<property name="version" column="b_version" />
		<property name="size" column="b_size" />
		<property name="content" column="b_content" />
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ccit.model.FDevice" table="t_f_device">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="name" column="d_name" />
		<property name="mac" column="d_mac" />
		<property name="indate" column="d_indate" />
		<property name="outdate" column="d_outdate" />
		<property name="status" column="d_status" />
	</class>
</hibernate-mapping>